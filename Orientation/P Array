#include <iostream>
using namespace std;

int main()
{
	int arr[10]; // 이 배열의 크기는 상수로 지정
	int sum = 0;

	for (int i = 0; i < sizeof(arr) / sizeof(arr[0]); i++)
	{
		cin >> arr[i];
		sum += arr[i];
	}

	cout << sum << endl;

	string str = "Hello"; // string 또한 char을 배열로 나타낸 것이다
	cout << str << endl;

	return 0;
}
